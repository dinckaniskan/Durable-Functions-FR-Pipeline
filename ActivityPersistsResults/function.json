{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "name",
      "type": "activityTrigger",
      "direction": "in"
    },
    {
      "name": "myblob",
      "type": "blobTrigger",      
      "path": "blob-trigger/{name}",
      "connection": "AzureBlobStorage",
      "direction": "out"
    }
  ]
}